{
  "daemon": "localhost_test",
  "log_name": "focusd@protodt",
  "control_machines": ["LocalHost"],
  "state_path": "/var/tmp/focuser.json",
  "serial_port": "/dev/serial/by-id/usb-Klipper_rp2040_454741505C0B99DA-if00",
  "serial_baud": 250000,
  "connect_timeout": 10,
  "home_timeout": 30,
  "move_timeout": 30,
  "controller_fan": {
    "pin": "gpio17",
    "idle_timeout": 30
  },
  "interfaces": {
    "tmc_uart": {
      "type": "tmc2209",
      "uart_pin": "gpio9",
      "tx_pin": "gpio8"
    }
  },
  "probes": {
    "1": {
      "label": "Thermistor 1",
      "cadence": 5,
      "type": "3950NTC",
      "pin": "gpio27"
    },
    "2": {
      "label": "Thermistor 2",
      "cadence": 5,
      "type": "3950NTC",
      "pin": "gpio26"
    },
    "mcu": {
      "label": "MCU",
      "cadence": 5,
      "type": "RP2040"
    }
  },
  "steppers": {
    "1": {
      "step_pin": "gpio19",
      "dir_pin": "gpio28",
      "enable_pin": "!gpio2",
      "rotation_microsteps": 3200,
      "rotation_distance": 0.75,
      "endstop_pin": "^gpio25",
      "position_min": 0,
      "position_max": 50,
      "speed": 2.5,
      "acceleration": 0.5,
      "homing_backoff": 1,
      "tracking_cadence": 0.1,
      "tracking_commit_buffer": 0.25,
      "interface": "tmc_uart",
      "uart_address": 1,
      "uart_microsteps": 16,
      "uart_run_current": 0.8
    },
    "2": {
      "step_pin": "gpio6",
      "dir_pin": "gpio5",
      "enable_pin": "!gpio7",
      "rotation_microsteps": 3200,
      "rotation_distance": 0.75,
      "endstop_pin": "^gpio3",
      "position_min": 0,
      "position_max": 50,
      "speed": 2.5,
      "acceleration": 0.5,
      "homing_backoff": 1,
      "tracking_cadence": 0.1,
      "tracking_commit_buffer": 0.25,
      "interface": "tmc_uart",
      "uart_address": 2,
      "uart_microsteps": 16,
      "uart_run_current": 0.8
    },
    "3": {
      "step_pin": "gpio11",
      "dir_pin": "gpio10",
      "enable_pin": "!gpio12",
      "rotation_microsteps": 3200,
      "rotation_distance": 0.75,
      "endstop_pin": "^gpio4",
      "position_min": 0,
      "position_max": 50,
      "speed": 2.5,
      "acceleration": 0.5,
      "homing_backoff": 1,
      "tracking_cadence": 0.1,
      "tracking_commit_buffer": 0.25,
      "interface": "tmc_uart",
      "uart_address": 0,
      "uart_microsteps": 16,
      "uart_run_current": 0.8
    },
    "4": {
      "step_pin": "gpio14",
      "dir_pin": "gpio13",
      "enable_pin": "!gpio15",
      "rotation_microsteps": 3200,
      "rotation_distance": 0.75,
      "endstop_pin": "^gpio16",
      "position_min": 0,
      "position_max": 50,
      "speed": 2.5,
      "acceleration": 0.5,
      "homing_backoff": 1,
      "tracking_cadence": 0.1,
      "tracking_commit_buffer": 0.25,
      "interface": "tmc_uart",
      "uart_address": 3,
      "uart_microsteps": 16,
      "uart_run_current": 0.8
    }
  }
}
