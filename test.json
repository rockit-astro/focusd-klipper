{
  "daemon": "localhost_test",
  "log_name": "focusd@portable",
  "control_machines": ["LocalHost"],
  "loop_delay": 5,
  "move_speed": 1,
  "move_timeout": 90,
  "max_position": 15,
  "homing_coarse_step": 5,
  "homing_coarse_speed": 1,
  "homing_fine_step": 0.5,
  "homing_fine_speed": 0.1,
  "mcu": {
    "serial_port": "/dev/serial/by-id/usb-Klipper_rp2040_454741505A8A24EA-if00",
    "serial_baud": 250000,
    "serial_timeout": 60,
    "tmc_uarts": {
      "shared": {
        "type": "tmc2209",
        "uart_pin": "gpio9",
        "tx_pin": "gpio8"
      }
    },
    "steppers": {
      "0": {
        "step_pin": "gpio11",
        "dir_pin": "!gpio10",
        "enable_pin": "!gpio12",
        "rotation_microsteps": 3200,
        "rotation_distance": 1,
        "endstop_pin": "^gpio4",
        "position_min": 0,
        "position_max": 50,
        "speed": 2.5,
        "acceleration": 0.5,
        "homing_backoff": 1,
        "tmc_uart": {
          "uart": "shared",
          "address": 0,
          "microsteps": 16,
          "run_current": 0.8
        }
      },
      "1": {
        "step_pin": "gpio6",
        "dir_pin": "!gpio5",
        "enable_pin": "!gpio7",
        "rotation_microsteps": 3200,
        "rotation_distance": 1,
        "endstop_pin": "^gpio4",
        "position_min": 0,
        "position_max": 50,
        "speed": 2.5,
        "acceleration": 0.5,
        "homing_backoff": 1,
        "tmc_uart": {
          "uart": "shared",
          "address": 2,
          "microsteps": 16,
          "run_current": 0.8
        }
      },
      "2": {
        "step_pin": "gpio19",
        "dir_pin": "!gpio28",
        "enable_pin": "!gpio2",
        "rotation_microsteps": 3200,
        "rotation_distance": 1,
        "endstop_pin": "^gpio4",
        "position_min": 0,
        "position_max": 50,
        "speed": 2.5,
        "acceleration": 0.5,
        "homing_backoff": 1,
        "tmc_uart": {
          "uart": "shared",
          "address": 1,
          "microsteps": 16,
          "run_current": 0.8
        }
      }
    }
  }
}