{
  "daemon": "halfmetre_focuser",
  "log_name": "focusd@halfmetre",
  "control_machines": ["HalfMetreTCS"],
  "state_path": "/var/tmp/focuser-halfmetre.json",
  "serial_port": "/dev/serial/by-id/usb-Klipper_stm32g0b1xx_340008000B50425539393020-if00",
  "serial_baud": 250000,
  "connect_timeout": 10,
  "home_timeout": 30,
  "move_timeout": 30,
  "interfaces": {
    "ds2484": {
      "type": "DS2484",
      "i2c_bus": "i2c3_PB3_PB4"
    },
    "tmc_uart": {
      "type": "tmc2209",
      "uart_pin": "PA15"
    }
  },
  "probes": {
    "primary_mirror": {
      "label": "Primary Mirror",
      "cadence": 5,
      "type": "DS18B20",
      "address": "28EFE61E0B0000B9",
      "interface": "ds2484"
    },
    "secondary_mirror": {
      "label": "Secondary Mirror",
      "cadence": 5,
      "type": "DS18B20",
      "address": "28FBE91E0B000051",
      "interface": "ds2484"
    }
  },
  "steppers": {
    "1": {
      "step_pin": "PD0",
      "dir_pin": "!PD1",
      "enable_pin": "!PD2",
      "rotation_microsteps": 3200,
      "rotation_distance": 0.635,
      "position_min": -50,
      "position_max": 50,
      "speed": 2.5,
      "acceleration": 0.5,
      "homing_backoff": 1,
      "tracking_cadence": 0.1,
      "tracking_commit_buffer": 0.25,
      "interface": "tmc_uart",
      "uart_address": 0,
      "uart_microsteps": 16,
      "uart_run_current": 0.65
    }
  }
}